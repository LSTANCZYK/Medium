{
  "logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "medium": {
    "webhooks": [
      {
        "name": "MyGet package added",
        "enabled": true,
        "token": "secret",
        "actions": [
          {
            "name": "Trigger Travis CI build",
            "url": "https://travis-ci.org",
            "request": {
              "source_type": "Branch",
              "source_name": "master"
            }
          }
        ],
        "triggers": [
          {
            "name": "Package added",
            "enabled": true,
            "provider": "myget",
            "type": "mygetpackageadded",
            "rules": {
            }
        }]
      }
    ]
  }
}
