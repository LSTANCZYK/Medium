{
  "webhooks": [
    {
      "name": "MyGet package added",
      "enabled": true,
      "endpoint": "run-travis-ci-builds",
      "token": "secret",
      "actions": [
        {
          "name": "Build Coolector.Api [develop]",
          "codename": "build-coolector.api-develop",
          "enabled": true,
          "url": "https://api.travis-ci.org/repo/noordwind%2FCoolector.Api/requests",
          "headers": {
            "Content-Type": "application/json",
            "Acccept": "application/json",
            "Travis-API-Version": "3",
            "Authorization": "token secret"
          },
          "request": { "branch": "develop" }
        },
        {
          "name": "Build Coolector.Services.Storage [develop]",
          "codename": "build-coolector.service.storage-develop",
          "enabled": true,
          "url": "https://api.travis-ci.org/repo/noordwind%2FCoolector.Services.Storage/requests",
          "headers": {
            "Content-Type": "application/json",
            "Acccept": "application/json",
            "Travis-API-Version": "3",
            "Authorization": "token secret"
          },
          "request": { "branch": "develop" }
        }
      ],
      "triggers": [
        {
          "name": "Package added",
          "enabled": true,
          "provider": "myget",
          "type": "mygetpackageadded",
          "actions": ["*"],
          "rules": {
            "packageIdentifier": {
              "value": "Coolector.Services.Users.Shared",
              "comparison": "equals"
            }
          }
      }]
    }
  ]
}
